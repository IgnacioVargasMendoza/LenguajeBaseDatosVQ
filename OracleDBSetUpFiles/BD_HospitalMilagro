--------------------------------------------------------
--  DDL for Function FIDE_CITAS_ACTIVAS_FN
--------------------------------------------------------

  CREATE OR REPLACE EDITIONABLE FUNCTION "HOSPIMILAGRO"."FIDE_CITAS_ACTIVAS_FN" (p_id_doctor NUMBER) RETURN NUMBER IS
    v_count NUMBER;
BEGIN
    SELECT COUNT(*) INTO v_count FROM FIDE_CITAS_TB WHERE ID_DOCTOR = p_id_doctor AND ACTIVO = 1;
    RETURN v_count;
END;

/
--------------------------------------------------------
--  DDL for Function FIDE_CITAS_TB_CITAS_ACTIVADAS_FN
--------------------------------------------------------

  CREATE OR REPLACE EDITIONABLE FUNCTION "HOSPIMILAGRO"."FIDE_CITAS_TB_CITAS_ACTIVADAS_FN" (
    P_ID_CITA NUMBER
) RETURN NUMBER IS
    V_ACTIVO NUMBER;
BEGIN
    SELECT ACTIVO INTO V_ACTIVO
    FROM FIDE_CITAS_TB
    WHERE ID_CITA = P_ID_CITA;
    RETURN V_ACTIVO;
END;

/
--------------------------------------------------------
--  DDL for Function FIDE_DOCTORES_TB_OBTENER_TELEFONO_FN
--------------------------------------------------------

  CREATE OR REPLACE EDITIONABLE FUNCTION "HOSPIMILAGRO"."FIDE_DOCTORES_TB_OBTENER_TELEFONO_FN" (
    P_ID_DOCTOR NUMBER
) RETURN VARCHAR2 IS
    V_TELEFONO VARCHAR2(15);
BEGIN
    SELECT TELEFONO INTO V_TELEFONO
    FROM FIDE_DOCTORES_TB
    WHERE ID_DOCTOR = P_ID_DOCTOR;

    RETURN V_TELEFONO;
END;

/
--------------------------------------------------------
--  DDL for Function FIDE_DOCTORES_TB_VERIFICAR_DISPONIBILIDAD_FN
--------------------------------------------------------

  CREATE OR REPLACE EDITIONABLE FUNCTION "HOSPIMILAGRO"."FIDE_DOCTORES_TB_VERIFICAR_DISPONIBILIDAD_FN" (
    p_id_doctor NUMBER, 
    p_fecha DATE, 
    p_hora_inicio DATE) 
    RETURN VARCHAR2 
IS
    v_count NUMBER;
BEGIN
    SELECT COUNT(*) INTO v_count
    FROM FIDE_DISPONIBILIDAD_TB
    WHERE ID_DOCTOR = p_id_doctor
    AND DIA = TO_CHAR(p_fecha, 'DAY')
    AND p_hora_inicio BETWEEN HORA_INICIO AND HORA_FIN;

    IF v_count > 0 THEN
        RETURN 'Disponible';
    ELSE
        RETURN 'No disponible';
    END IF;
END;

/
--------------------------------------------------------
--  DDL for Function FIDE_FACTURAS_TB_OBTENER_MONTO_TOTAL_FN
--------------------------------------------------------

  CREATE OR REPLACE EDITIONABLE FUNCTION "HOSPIMILAGRO"."FIDE_FACTURAS_TB_OBTENER_MONTO_TOTAL_FN" (
    P_ID_FACTURA NUMBER
) RETURN NUMBER IS
    V_MONTO_TOTAL NUMBER(10,2);
BEGIN
    SELECT MONTO_TOTAL INTO V_MONTO_TOTAL
    FROM FIDE_FACTURAS_TB
    WHERE ID_FACTURA = P_ID_FACTURA;

    RETURN V_MONTO_TOTAL;
END;

/
--------------------------------------------------------
--  DDL for Function FIDE_PACIENTES_TB_MAX_CITAS_FN
--------------------------------------------------------

  CREATE OR REPLACE EDITIONABLE FUNCTION "HOSPIMILAGRO"."FIDE_PACIENTES_TB_MAX_CITAS_FN" RETURN NUMBER IS
    v_id_paciente NUMBER;
BEGIN
    SELECT ID_PACIENTE INTO v_id_paciente
    FROM (SELECT ID_PACIENTE, COUNT(*) AS TOTAL_CITAS
          FROM FIDE_CITAS_TB
          GROUP BY ID_PACIENTE
          ORDER BY TOTAL_CITAS DESC)
    WHERE ROWNUM = 1;
    RETURN v_id_paciente;
END;

/
--------------------------------------------------------
--  DDL for Function FIDE_TRATAMIENTOS_TB_TIENE_ACTIVO_FN
--------------------------------------------------------

  CREATE OR REPLACE EDITIONABLE FUNCTION "HOSPIMILAGRO"."FIDE_TRATAMIENTOS_TB_TIENE_ACTIVO_FN" (p_id_paciente NUMBER) RETURN VARCHAR2 IS
    v_count NUMBER;
BEGIN
    SELECT COUNT(*) INTO v_count
    FROM FIDE_TRATAMIENTOS_TB
    WHERE ID_PACIENTE = p_id_paciente AND ACTIVO = 1;

    IF v_count > 0 THEN
        RETURN 'S ';
    ELSE
        RETURN 'No';
    END IF;
END;

/
--------------------------------------------------------
--  DDL for Function FIDE_USUARIO_ROL_TB_CONTAR_USUARIOS_FN
--------------------------------------------------------

  CREATE OR REPLACE EDITIONABLE FUNCTION "HOSPIMILAGRO"."FIDE_USUARIO_ROL_TB_CONTAR_USUARIOS_FN" (
    P_ID_ROL NUMBER
) RETURN NUMBER IS
    V_TOTAL NUMBER;
BEGIN
    SELECT COUNT(*) INTO V_TOTAL
    FROM FIDE_USUARIO_ROL_TB
    WHERE ID_ROL = P_ID_ROL;

    RETURN V_TOTAL;
END;

