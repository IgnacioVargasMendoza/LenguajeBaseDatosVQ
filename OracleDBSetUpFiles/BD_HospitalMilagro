--------------------------------------------------------
--  DDL for View V_CITAS_DETALLE
--------------------------------------------------------

  CREATE OR REPLACE FORCE EDITIONABLE VIEW "HOSPIMILAGRO"."V_CITAS_DETALLE" ("ID_CITA", "FECHA", "HORA_CITA", "PACIENTE", "DOCTOR", "ACTIVO") AS 
  SELECT C.ID_CITA, C.FECHA, C.HORA_CITA, P.ID_PACIENTE AS PACIENTE, D.NUMEROLICENCIA AS DOCTOR, C.ACTIVO
FROM FIDE_CITAS_TB C
JOIN FIDE_PACIENTES_TB P ON C.ID_PACIENTE = P.ID_PACIENTE
JOIN FIDE_DOCTORES_TB D ON C.ID_DOCTOR = D.ID_DOCTOR
WHERE C.ACTIVO = 1
;
--------------------------------------------------------
--  DDL for View V_COSTOS_ACTIVOS
--------------------------------------------------------

  CREATE OR REPLACE FORCE EDITIONABLE VIEW "HOSPIMILAGRO"."V_COSTOS_ACTIVOS" ("ID_COSTO", "MONTO", "DESCRIPCION") AS 
  SELECT C.ID_COSTO, C.MONTO, C.DESCRIPCION
FROM FIDE_COSTOS_CITA_TB C
WHERE C.ACTIVO = 1
;
--------------------------------------------------------
--  DDL for View V_DISPONIBILIDAD_DOCTORES
--------------------------------------------------------

  CREATE OR REPLACE FORCE EDITIONABLE VIEW "HOSPIMILAGRO"."V_DISPONIBILIDAD_DOCTORES" ("ID_DOCTOR", "DIA", "HORA_INICIO", "HORA_FIN") AS 
  SELECT D.ID_DOCTOR, DISP.DIA, DISP.HORA_INICIO, DISP.HORA_FIN
FROM FIDE_DOCTORES_TB D
JOIN FIDE_DISPONIBILIDAD_TB DISP ON D.ID_DOCTOR = DISP.ID_DOCTOR
WHERE DISP.ACTIVO = 1
;
--------------------------------------------------------
--  DDL for View V_ESPECIALIDADES_ACTIVAS
--------------------------------------------------------

  CREATE OR REPLACE FORCE EDITIONABLE VIEW "HOSPIMILAGRO"."V_ESPECIALIDADES_ACTIVAS" ("ID_ESPECIALIDAD", "NOMBRE") AS 
  SELECT ID_ESPECIALIDAD, NOMBRE
FROM FIDE_ESPECIALIDADES_TB
WHERE ACTIVO = 1
;
--------------------------------------------------------
--  DDL for View V_HISTORIAL_CITAS
--------------------------------------------------------

  CREATE OR REPLACE FORCE EDITIONABLE VIEW "HOSPIMILAGRO"."V_HISTORIAL_CITAS" ("ID_HISTORIAL", "ID_CITA", "ESTADO") AS 
  SELECT H.ID_HISTORIAL, H.ID_CITA, E.NOMBRE_ESTADO AS ESTADO
FROM FIDE_HISTORIAL_CITAS_TB H
JOIN FIDE_ESTADOS_CITA_TB E ON H.ID_ESTADO_NUEVO = E.ID_ESTADO
WHERE H.ACTIVO = 1
;
--------------------------------------------------------
--  DDL for View V_PROCEDIMIENTOS
--------------------------------------------------------

  CREATE OR REPLACE FORCE EDITIONABLE VIEW "HOSPIMILAGRO"."V_PROCEDIMIENTOS" ("ID_PROCEDIMIENTO", "NOMBRE_PROCEDIMIENTO", "DESCRIPTION", "TIPO_PROCEDIMIENTO", "RIESGO") AS 
  SELECT ID_PROCEDIMIENTO, NOMBRE_PROCEDIMIENTO, DESCRIPTION, TIPO_PROCEDIMIENTO, RIESGO
FROM FIDE_PROCEDIMIENTOS_TB
WHERE ACTIVO = 1
;
